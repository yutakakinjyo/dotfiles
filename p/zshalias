alias p='eval $(cat ~/dotfiles/p/shm | sort | peco)'
alias s='ls ~/dotfiles/p | peco | xargs find ~/dotfiles/p -name | xargs cat | peco'
alias pe='emacs ~/dotfiles/p/"$(ls ~/dotfiles/p | peco)"'
alias pc='cd ~/dotfiles'
alias co='git br | peco | sed -e "s/\* //g" | awk "{print $1}" | xargs git co'
alias b='peco ~/dotfiles/p/html | xargs open'
alias pst='cd ~/dotfiles; git st; cd -'
alias pdiff='cd ~/dotfiles; git diff; cd -'
alias ppush='cd ~/dotfiles; git ci -am "update"; git push origin master; cd -'

